{
	"include": ["src/**/*.ts"],
	"compilerOptions": {
		"module": "ESNext",
		"target": "ESNext",
		"moduleResolution": "node",
		"noEmit": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"checkJs": true
	}
}
